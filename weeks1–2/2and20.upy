#nb python code example calculate formula annual value return
value_start = 10e6
print("start value %.2f" % value_start)

rate = 0.02
print("rate %.2f" % rate)

ret = 0.10
print("return %.2f" % ret)

rate_ret = 0.20
print("rate for return %.2f" % rate_ret)

commission = value_start * rate + value_start * ret * rate_ret
print("commission %.2f" % commission)
